{
  "name" : "read.crx 2",
  "version" : "0.71",
  "update_url": "http://idawef.com/crx/crx.xml",
  "homepage_url": "http://idawef.com/read.crx-2/",
  "description" : "2chブラウザ",
  "manifest_version": 2,
  "minimum_chrome_version" : "23",
  "content_security_policy": "default-src 'self'; img-src 'self' http://* https://* data:; style-src 'self' 'unsafe-inline'; connect-src 'self' http://*; frame-src 'self' http://*; font-src data:",
  "incognito" : "split",
  "options_page" : "view/index.html?q=config",
  "permissions" : [
    "bookmarks",
    "cookies",
    "tabs",
    "storage",
    "unlimitedStorage",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/*"
  ],
  "app" : {
    "launch" : {
      "local_path" : "/view/index.html"
    }
  },
  "icons" : {
    "128" : "img/read.crx_128x128.png",
    "48" : "img/read.crx_48x48.png",
    "16" : "img/read.crx_16x16.png"
  },
  "content_scripts" : [
    {
      "matches" : [
        "http://*.2ch.net/*",
        "http://jbbs.livedoor.jp/*",
        "http://*.machi.to/*"
      ],
      "js" : ["/cs_addlink.js"]
    },
    {
      "matches" : [
        "http://*.2ch.net/test/bbs.cgi*",
        "http://jbbs.livedoor.jp/bbs/write.cgi/*",
        "http://*.p2.2ch.net/p2/post_form.php?*",
        "http://*.p2.2ch.net/p2/post.php?*"
      ],
      "js" : ["/write/cs_write.js"],
      "run_at" : "document_end",
      "all_frames" : true
    }
  ],
  "web_accessible_resources": [
    "/view/board.html",
    "/view/bookmark.html",
    "/view/bookmark_source_selector.html",
    "/view/config.html",
    "/view/empty.html",
    "/view/history.html",
    "/view/inputurl.html",
    "/view/search.html",
    "/view/sidemenu.html",
    "/view/thread.html",
    "/test/message_test.html"
  ]
}
